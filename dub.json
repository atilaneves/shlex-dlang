        },
        {
          "name": "static-library",
          "targetType": "staticLibrary"
        }
      ],
      "dependencies": {
        "pure-dependency-injector": "~>0.9.5"	
      }
    },
    {
      "name": "shlex",
      "description": "Shlex lexer (test)",
      "targetName": "shlex",
      "configurations": [
        {
          "name": "application",
          "targetType": "executable",
          "sourceFiles": [
            "shlex-main.d"
          ]
        }
      ],
      "dependencies": {
        "shlex:library": "*",
      }
    }
  ],
  "dependencies": {
    "shlex:library": "*",
    "shlex:shlex": "*",
  }
}
