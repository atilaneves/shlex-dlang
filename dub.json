{
  "name": "shlex",
  "description": "Simple shell-like lexical analysis library",
  "homepage": "https://github.com/vporton/shlex-dlang",
  "license": "GPL-3.0 or later",
  "authors": [
    "Victor Porton"
  ],
  "targetType": "none",
  "subPackages": [
    {
      "name": "library",
      "description": "Shlex library",
      "targetName": "shlex",
      "configurations": [
        {
          "name": "application",
          "targetType": "executable",
          "sourceFiles": [
            "test.d"
          ]
        },
        {
          "name": "shared-library",
